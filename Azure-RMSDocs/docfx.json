{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**"
        ],
        "src": "articles",
        "dest": "."
      },
      {
      "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**"
        ],
        "src": "bread",
        "dest": "bread"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": "articles",
        "dest": "."
      },
      {
        "files": [
          "includes/**/*.svg",
          "includes/**/*.png",
          "includes/**/*.jpg",
          "includes/**/*.jpeg",
          "includes/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "dest": "Azure-RMSDocs",
    "template": "docs.html",
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/azure-rmsdocs",
      "feedback_product_url": "https://msip.uservoice.com/", 
      "layout": "Conceptual",
      "breadcrumb_path": "/azure/information-protection/bread/toc.json",
      "searchScope": ["Information Protection"]
    }
  }
}
